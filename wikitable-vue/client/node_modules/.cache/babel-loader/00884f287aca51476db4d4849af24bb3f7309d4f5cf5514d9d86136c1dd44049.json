rn new Date(timestamp * 1000).toLocaleString();\r\n\t};\r\n\r\n\tconst splitIntoSentences = text => {\r\n\t\tif (!text) return [];\r\n\t\tconst regex = /([^。！？.!?]+[。！？.!?]+)/g;\r\n\t\treturn [...text.matchAll(regex)].map(m => m[0].trim()).filter(Boolean);\r\n\t};\r\n\r\n\tconst getCommentsForSentence = index => {\r\n\t\treturn comments.value.filter(\r\n\t\t\tc => c?.link === index && visibleCommentTypes.value[getCommentType(c)]\r\n\t\t);\r\n\t};\r\n\r\n\tconst hasComments = index => getCommentsForSentence(index).length > 0;\r\n\tconst getTopComment = index => {\r\n\t\tconst comments = getCommentsForSentence(index);\r\n\t\treturn comments.length\r\n\t\t\t? [...comments].sort((a, b) => b.like_count - a.like_count)[0]\r\n\t\t\t: null;\r\n\t};\r\n\r\n\tconst getCommentLevel = index => {\r\n\t\tconst count = getCommentsForSentence(index).length;\r\n\t\treturn {\r\n\t\t\tlevel: Math.min(Math.floor(count / 3) + 1, 5),\r\n\t\t\ttype: getCommentType(getTopComment(index))\r\n\t\t};\r\n\t};\r\n\r\n\tconst handleAvatarError = e => {\r\n\t\te.target.src = defaultAvatar;\r\n\t};\r\n\r\n\tconst handleWordClick = ({ word, frequency }) => {\r\n\t\tconsole.log(`高频词 \"${word}\" 被点击，出现 ${frequency} 次`);\r\n\t};\r\n\r\n\t// 初始化\r\n\tonMounted(() => {\r\n\t\tapi.get(\"article\", {}, response => {\r\n\t\t\tarticle.value = response?.data?.[0] || response?.[0];\r\n\t\t\tif (article.value?.content_text) {\r\n\t\t\t\tsentences.value = splitIntoSentences(article.value.content_text);\r\n\t\t\t}\r\n\t\t});\r\n\r\n\t\tapi.get(\"comments\", {}, response => {\r\n\t\t\tcomments.value = response?.data || response || [];\r\n\t\t});\r\n\t});\r\n</script>\r\n\r\n<style scoped>\r\n\t.article-container {\r\n\t\tdisplay: flex;\r\n\t\tflex-direction: column;\r\n\t\tmax-width: 1800px;\r\n\t\tmargin: 0 auto;\r\n\t\tpadding: 20px;\r\n\t\tgap: 15px;\r\n\t}\r\n\r\n\t.control-panel {\r\n\t\tdisplay: flex;\r\n\t\tjustify-content: space-between;\r\n\t\talign-items: center;\r\n\t\tgap: 15px;\r\n\t\tpadding: 12px 15