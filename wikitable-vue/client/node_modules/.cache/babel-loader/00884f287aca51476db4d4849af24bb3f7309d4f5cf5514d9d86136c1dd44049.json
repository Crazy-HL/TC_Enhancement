{"ast":null,"code":"import { ref, computed, onMounted, watch, nextTick } from \"vue\";\nimport api from \"@/api\";\nimport HighFrequencyWords from \"./HighFrequencyWords.vue\";\nimport CommentStats from \"./CommentStats.vue\";\nconst defaultAvatar = \"data:image/svg+xml;base64,\";\n\n// 响应式数据\n\nexport default {\n  __name: 'ArticleContent',\n  setup(__props, _ref) {\n    let {\n      expose: __expose\n    } = _ref;\n    __expose();\n    const article = ref(null);\n    const sentences = ref([]);\n    const comments = ref([]);\n    const activeSentence = ref(null);\n    const showInlineComments = ref(true);\n    const showPieCharts = ref(true);\n    const showContentHighlight = ref(true);\n    const showHighFrequencyWords = ref(true);\n    const visibleCommentTypes = ref({\n      1: true,\n      2: true,\n      3: true,\n      4: true,\n      5: true\n    });\n\n    // 计算属性\n    const allCommentsForLegend = computed(() => comments.value);\n    const globalComments = computed(() => {\n      return comments.value.filter(c => c?.link === -1);\n    });\n\n    // 方法\n    const toggleCommentTypeVisibility = type => {\n      visibleCommentTypes.value[type] = !visibleCommentTypes.value[type];\n    };\n    const toggleActiveSentence = index => {\n      activeSentence.value = activeSentence.value === index ? null : index;\n    };\n    const getCommentType = comment => comment?.comment_type || 1;\n    const getCommentTypeLabel = comment => {\n      const types = {\n        1: \"陈述\",\n        2: \"疑问\",\n        3: \"感叹\",\n        4: \"建议\",\n        5: \"讽刺\"\n      };\n      return types[getCommentType(comment)] || \"未知\";\n    };\n    const formatNumber = num => {\n      if (!num && num !== 0) return \"0\";\n      return num >= 1000 ? (num / 1000).toFixed(1) + \"k\" : num;\n    };\n    const formatTime = timestamp => {\n      return new Date(timestamp * 1000).toLocaleString();\n    };\n    const splitIntoSentences = text => {\n      if (!text) return [];\n      const regex = /([^。！？.!?]+[。！？.!?]+)/g;\n      return [...text.matchAll(regex)].map(m => m[0].trim()).filter(Boolean);\n    };\n    const getCommentsForSentence = index => {\n      return comments.value.filter(c => c?.link === index && visibleCommentTypes.value[getCommentType(c)]);\n    };\n    const hasComments = index => getCommentsForSentence(index).length > 0;\n    const getTopComment = index => {\n      const comments = getCommentsForSentence(index);\n      return comments.length ? [...comments].sort((a, b) => b.like_count - a.like_count)[0] : null;\n    };\n    const getCommentLevel = index => {\n      const count = getCommentsForSentence(index).length;\n      return {\n        level: Math.min(Math.floor(count / 3) + 1, 5),\n        type: getCommentType(getTopComment(index))\n      };\n    };\n    const handleAvatarError = e => {\n      e.target.src = defaultAvatar;\n    };\n    const handleWordClick = _ref2 => {\n      let {\n        word,\n        frequency\n      } = _ref2;\n      console.log(`高频词 \"${word}\" 被点击，出现 ${frequency} 次`);\n    };\n\n    // 初始化\n    onMounted(() => {\n      api.get(\"article\", {}, response => {\n        article.value = response?.data?.[0] || response?.[0];\n        if (article.value?.content_text) {\n          sentences.value = splitIntoSentences(article.value.content_text);\n        }\n      });\n      api.get(\"comments\", {}, response => {\n        comments.value = response?.data || response || [];\n      });\n    });\n    const __returned__ = {\n      defaultAvatar,\n      article,\n      sentences,\n      comments,\n      activeSentence,\n      showInlineComments,\n      showPieCharts,\n      showContentHighlight,\n      showHighFrequencyWords,\n      visibleCommentTypes,\n      allCommentsForLegend,\n      globalComments,\n      toggleCommentTypeVisibility,\n      toggleActiveSentence,\n      getCommentType,\n      getCommentTypeLabel,\n      formatNumber,\n      formatTime,\n      splitIntoSentences,\n      getCommentsForSentence,\n      hasComments,\n      getTopComment,\n      getCommentLevel,\n      handleAvatarError,\n      handleWordClick,\n      ref,\n      computed,\n      onMounted,\n      watch,\n      nextTick,\n      get api() {\n        return api;\n      },\n      HighFrequencyWords,\n      CommentStats\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","watch","nextTick","api","HighFrequencyWords","CommentStats","defaultAvatar","article","sentences","comments","activeSentence","showInlineComments","showPieCharts","showContentHighlight","showHighFrequencyWords","visibleCommentTypes","allCommentsForLegend","value","globalComments","filter","c","link","toggleCommentTypeVisibility","type","toggleActiveSentence","index","getCommentType","comment","comment_type","getCommentTypeLabel","types","formatNumber","num","toFixed","formatTime","timestamp","Date","toLocaleString","splitIntoSentences","text","regex","matchAll","map","m","trim","Boolean","getCommentsForSentence","hasComments","length","getTopComment","sort","a","b","like_count","getCommentLevel","count","level","Math","min","floor","handleAvatarError","e","target","src","handleWordClick","_ref2","word","frequency","console","log","get","response","data","content_text"],"sources":["D:/ZhiHu/zhihu-bullet/wikitable-vue/client/src/components/ArticleContent.vue"],"sourcesContent":["<template>\r\n\t<div class=\"article-container\">\r\n\t\t<div class=\"control-panel\">\r\n\t\t\t<div class=\"control-group-container\">\r\n\t\t\t\t<div class=\"control-group\">\r\n\t\t\t\t\t<label class=\"icon-toggle\">\r\n\t\t\t\t\t\t<input type=\"checkbox\" v-model=\"showInlineComments\" />\r\n\t\t\t\t\t\t<svg viewBox=\"0 0 24 24\" width=\"18\" height=\"18\">\r\n\t\t\t\t\t\t\t<path\r\n\t\t\t\t\t\t\t\td=\"M12,3C17,3 21,7 21,12C21,17 17,21 12,21C7,21 3,17 3,12C3,7 7,3 12,3M12,7C10.9,7 10,7.9 10,9C